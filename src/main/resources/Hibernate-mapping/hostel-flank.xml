<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="oltiv.business.HostelFlank" table="Hostel_flank">
      <meta attribute="class-description">
         This class contains the Hostel detail.
      </meta>
      <id name="id" type="int" column="id">
         <generator class="increment"/>
      </id>
      <property name="flankName" column="flankName" type="string"/>
      <property name="hostelId" column="hostelId" type="int"/>
      <property name="noOfRooms" column="NoOfRooms" type="int"/>
      <property name="columns" column="columns" type="int"/>
      <property name="studenPerRoom" column="studentPerRoom" type="int"/>
   </class>
</hibernate-mapping>
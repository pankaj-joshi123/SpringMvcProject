<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="oltiv.business.Prapti" table="Prapti">
      <meta attribute="class-description">
         This class contains the Prapti's Boyfriend detail.
      </meta>
      <id name="id" type="int" column="id">
         <generator class="increment"/>
      </id>
      <property name="boyFrndName" column="name" type="string"/>
      <property name="boyFrndLastName" column="lastName" type="string"/>
   </class>
</hibernate-mapping>
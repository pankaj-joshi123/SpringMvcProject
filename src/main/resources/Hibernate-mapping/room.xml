<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="oltiv.business.FlankRooms" table="Room">
      <meta attribute="class-description">
         This class contains the Hostel detail.
      </meta>
      <id name="id" type="int" column="id">
         <generator class="increment"/>
      </id>
      <property name="roomLabel" column="roomLabel" type="string"/>
      <property name="noofStudentsPresent" column="noofStudentsPresent" type="int"/>
      <property name="flankId" column="flankId" type="int"/>
      <property name="capacityOfRoom" column="capacityOfRoom" type="int"/>
   </class>
</hibernate-mapping>